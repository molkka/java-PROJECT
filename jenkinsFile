pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat 'mvn clean package'
            }
        }
        stage('SonarQube Analysis') {
            environment {
                scannerHome = tool 'SonarQube Scanner'
            }
            steps {
                bat "${scannerHome}/bin/sonar-scanner.bat -Dsonar.projectKey=your_project_key"
            }
        }
    }
}
