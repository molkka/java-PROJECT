pipeline {
        agent none
tools {
        maven 'Maven'  // Define the Maven tool
    }
        stages {
         
          stage("build & SonarQube Scanner") {
            agent any
            steps {
              withSonarQubeEnv('SonarQube') {
                bat 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }
